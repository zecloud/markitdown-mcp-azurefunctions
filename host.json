{
  "version": "2.0",
  "logging": {
    "applicationInsights": {
      "samplingSettings": {
        "isEnabled": true,
        "maxTelemetryItemsPerSecond": 20
      }
    }
  },
  "extensions": {
        "http": {
            "routePrefix": ""
        }
    },
    "customHandler": {
        "description": {
            "defaultExecutablePath": "python",
            "workingDirectory": "",
            "arguments": ["-m","markitdown-mcp","--http","--host","127.0.0.1","--port","%FUNCTIONS_CUSTOMHANDLER_PORT%"]
        },
        "enableForwardingHttpRequest": true,
        "enableHttpProxyingRequest": true
    }
}
